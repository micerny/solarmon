[influx]
host = localhost
port = 8086
db_name = home
measurement = inverter

[prometheus]
postUri = https://influx-prod-10-prod-us-central-0.grafana.net/api/v1/push/influx/write
token = <your Bearer token for Prometheus, without the prefix "Bearer ">

[query]
interval = 10
error_interval = 60

[growatt]
unit = 1
port = /dev/ttyUSB0

[greenbono]
host = 192.168.0.100
port = 6770
voltage_ratio = 1.0389
