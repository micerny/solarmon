[influx]
host = localhost
port = 8086
db_name = home

[prometheus]
postUri = https://influx-prod-10-prod-us-central-0.grafana.net/api/v1/push/influx/write
token = <your Bearer token form Prometheus, without the prefix "Bearer "> 

[query]
interval = 5
offline_interval = 60
error_interval = 60

[solarmon]
port = /dev/ttyUSB0

[inverters.growatt]
unit = 1
measurement = inverter
